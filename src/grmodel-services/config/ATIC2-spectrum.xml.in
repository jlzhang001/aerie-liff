      <energyRange unit="GeV"> 1e2 1e5 </energyRange>
      <normF unit="1e-5/(GeV*meter2*s*sr)"> 1.348956 </normF>
      <normE unit="GeV"> 1e3 </normE>
      <index> -2.75 </index>
    </spectrum>
  </source>

  <!-- ========================== Oxygen spectrum ========================== -->
