<?xml version="1.0"?>

<!-- Energy spectra of various primary species: power law fits using data from
     the ATIC-2 balloon flights.
     -->

<cosmicRayModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="@HAWC_CONFIG@/CRSpectrum.xsd">

  <!-- ========================== Proton spectrum ========================== -->
  <!-- A.D. Panov et al., Bull. Russ. Acad. Sci. 73 (2009) 564-567 -->
  <source>
    <name> ATIC2ProtonFit </name>
    <particleType> Proton </particleType>
    <spectrum type="PowerLaw">
      <energyRange unit="GeV"> 1e2 1e5 </energyRange>
      <normF unit="1e-5/(GeV*meter2*s*sr)"> 8.8641 </normF>
      <normE unit="GeV"> 1e3 </normE>
      <index> -2.65 </index>
    </spectrum>
  </source>

  <!-- ========================== Helium spectrum ========================== -->
  <!-- A.D. Panov et al., Bull. Russ. Acad. Sci. 73 (2009) 564-567 -->
  <source>
    <name> ATIC2HeliumFit </name>
    <particleType> He4 </particleType>
    <spectrum type="PowerLaw">
      <energyRange unit="GeV"> 1e2 1e5 </energyRange>
      <normF unit="1e-5/(GeV*meter2*s*sr)"> 6.448 </normF>
      <normE unit="GeV"> 1e3 </normE>
      <index> -2.60 </index>
    </spectrum>
  </source>

  <!-- ========================== Carbon spectrum ========================== -->
  <!-- A.D. Panov et al., Bull. Russ. Acad. Sci. 73 (2009) 564-567 -->
