      <normE unit="GeV"> 1e3 </normE>
      <index> -2.75 </index>
    </spectrum>
  </source>

  <!-- ========================= Magnesium spectrum ======================== -->
  <!-- A.D. Panov et al., Bull. Russ. Acad. Sci. 73 (2009) 564-567 -->
