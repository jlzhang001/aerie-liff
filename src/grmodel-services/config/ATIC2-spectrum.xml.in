    <name> ATIC2MagnesiumFit </name>
    <particleType> Mg24 </particleType>
    <spectrum type="PowerLaw">
      <energyRange unit="GeV"> 1e2 1e5 </energyRange>
      <normF unit="1e-5/(GeV*meter2*s*sr)"> 0.915 </normF>
      <normE unit="GeV"> 1e3 </normE>
      <index> -2.75 </index>
    </spectrum>
  </source>

  <!-- ========================== Silicon spectrum ========================= -->
  <!-- A.D. Panov et al., Bull. Russ. Acad. Sci. 73 (2009) 564-567 -->
